{
    "name": "lrc-maker",
    "version": "5.0.0-alpha.20190117",
    "description": "歌词滚动姬｜可能是你所能见到的最好用的歌词制作工具",
    "repository": "https://github.com/magic-akari/lrc-maker.git",
    "author": "阿卡琳 <hufan.akarin@Gmail.com>",
    "license": "MIT",
    "keywords": [
        "lrc",
        "lrc maker",
        "lyric",
        "lyric maker"
    ],
    "bugs": "https://github.com/magic-akari/lrc-maker/issues",
    "homepage": "https://github.com/magic-akari/lrc-maker#readme",
    "files": [
        "build"
    ],
    "scripts": {
        "start": "npm run html && npm run tsc:watch",
        "build": "npm run css && npm run tsc:build && npm run html",
        "tsc": "tsc",
        "tsc:build": "tsc -b",
        "tsc:watch": "tsc -w",
        "css": "postcss src/index.css -m -d build",
        "html": "ts-node -P scripts/tsconfig.json scripts/index.html.tsx > build/index.html",
        "prehtml": "mkdir -p build",
        "prettier": "prettier --write \"./{src,worker,scripts}/**/*.{ts,tsx,js,css,json}\" \"./*.{js,json,md}\" ",
        "lint": "tslint {src,worker,scripts}/**/*.{ts,tsx}"
    },
    "dependencies": {
        "dialog-polyfill": "^0.4.10",
        "normalize.css": "^8.0.1",
        "react": "^16.8.0-alpha.0",
        "react-dom": "^16.8.0-alpha.0"
    },
    "devDependencies": {
        "@types/node": "^10.12.18",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "autoprefixer": "^9.4.5",
        "cssnano": "^4.1.8",
        "fast-glob": "^2.2.6",
        "postcss": "^7.0.11",
        "postcss-cli": "^6.1.1",
        "postcss-custom-properties": "^8.0.9",
        "postcss-import": "^12.0.1",
        "prettier": "^1.15.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.1",
        "typescript": "^3.3.0-dev.20190112"
    }
}
